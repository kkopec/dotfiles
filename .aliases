# edition and reload of .zshrc
alias vz="$EDITOR ~/.zshrc"
alias rz=". ~/.zshrc"

alias repo="cd ~/repo"

alias calc="echo $@ | bc -l"

# @see .scripts
alias grepd="~/.scripts/grepd.sh $@"
alias cdf=". ~/.scripts/cddir.sh $@"
alias cdfs=". ~/.scripts/cddir.sh ~ $@"

# @see .functions
alias mkcd=mkcd_func
alias cds=cdls

alias otw=overthewire
